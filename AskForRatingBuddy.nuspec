		<file src="AskForRatingBuddy.WindowsDX\bin\Release\AskForRatingBuddy.WindowsDX.pdb" target="lib\net451\AskForRatingBuddy.WindowsDX.pdb" />
		<file src="AskForRatingBuddy.WindowsDX\bin\Release\AskForRatingBuddy.WindowsDX.dll" target="lib\net451\AskForRatingBuddy.WindowsDX.dll" />

