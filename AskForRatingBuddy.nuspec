<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
	<metadata>
		<id>AskForRatingBuddy</id>
		<version>2.0.8</version>
		<title>AskForRatingBuddy</title>
		<authors>dmanning23</authors>
		<owners>dmanning23</owners>
		<projectUrl>https://github.com/dmanning23/AskForRatingBuddy</projectUrl>
		<licenseUrl>https://github.com/dmanning23/AskForRatingBuddy/blob/master/LICENSE.txt</licenseUrl>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>MonoGame library for periodically asking the user to rate your app on the Google Play Store and Apple App Store</description>
		<releaseNotes />
		<copyright>Copyright 2016</copyright>
		<tags>pcl, windows phone, winphone, android, ios, windows universal, monogame</tags>
